{
  "name": "ca",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env APP_ENV=dev node ./dist/src/index.js",
    "build": "tsoa spec-and-routes && tsc && tsc-alias",
    "dev": "cross-env APP_ENV=dev concurrently \"nodemon\" \"nodemon --config nodemon.routes.json\"",
    "test": "cross-env APP_ENV=test vitest --ui",
    "test:ui": "cross-env APP_ENV=test vitest --ui --open",
    "test:e2e": "cross-env APP_ENV=test vitest run --ui --config ./vitest.config.ts",
    "test:application": "cross-env APP_ENV=test vitest watch --ui --project application",
    "test:domain": "cross-env APP_ENV=test vitest watch --ui --project domain",
    "test:infrastructure": "cross-env APP_ENV=test vitest --ui --project infrastructure",
    "type-check": "tsc --noEmit",
    "lint": "npx eslint .",
    "start-dev-env": "cross-env APP_ENV=dev docker compose -f docker-compose.dev.yml up ",
    "start-test-env": "cross-env APP_ENV=test docker compose -f docker-compose.test.yml up "
  },
